    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/platform-server": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@popperjs/core": "^2.11.8",
    "@stripe/stripe-js": "^3.4.1",
    "@types/stripe": "^8.0.417",
    "bootstrap": "^5.3.3",
    "jquery": "^3.7.1",
    "ngx-stripe": "^18.0.0",
    "oidc-client": "^1.11.5",
